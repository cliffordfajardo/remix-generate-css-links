{
  "name": "remix-generate-css-links",
  "version": "0.1.4",
  "description": "In your Remix.run project, automatically generate links for your imported .css files. You can import css the way you do in regular React projects and still get the benefits of exporting links in your Remix Run project the remix way.",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "bin": {
    "remix-generate-css-links": "lib/cli.js"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "prepack": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neighbaa/remix-generate-css-links.git"
  },
  "keywords": [
    "remix",
    "remix.run",
    "generate",
    "css",
    "links"
  ],
  "author": "Julian Hoben",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neighbaa/remix-generate-css-links/issues"
  },
  "homepage": "https://github.com/neighbaa/remix-generate-css-links#readme",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "chokidar": "^3.5.3",
    "dependency-tree": "^8.1.2",
    "fs-extra": "^10.0.0",
    "meow": "9.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "typescript": "^4.5.5",
    "concurrently": "^6.x",
    "ts-node": "^10.4.0"
  }
}
